                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="redisExpireProperties" class="org.springframework.beans.factory.config.PropertiesFactoryBean">
        <property name="locations" value="classpath*:conf/cache/redis/redis-expire.properties" />
    </bean>

</beans>
